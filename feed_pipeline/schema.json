{
  "settings": {
    "analysis": {
      "analyzer": {
        "default": {
          "tokenizer": "whitespace" ,
          "filter": ["my_word_delimiter_index", "lowercase", "my_stemming", "asciifolding", "stop_english", "stop_custom", "shingle_filter"]
        },
        "default_search": {
          "tokenizer": "whitespace",
          "filter": ["my_word_delimiter_search", "lowercase", "my_stemming", "asciifolding", "stop_english", "stop_custom", "shingle_filter"]
        },
        "shingle_analyzer": {
          "tokenizer": "whitespace",
          "filter": ["my_word_delimiter_index", "lowercase", "synonym_indextime",  "my_stemming", "asciifolding", "stop_english", "stop_custom", "shingle_filter"]
        }
      },
      "normalizer":{
        "case_insensitive_sort": {
          "type": "custom",
          "char_filter": ["remove_space"],
          "filter": ["lowercase", "asciifolding"]
        }
      },
      "char_filter":{
        "remove_space": {
          "type": "pattern_replace",
          "pattern": "(\\s)",
          "replacement": " "
        }
      },
      "filter": {
        "my_word_delimiter_index": {
          "type": "word_delimiter",
          "catenate_numbers": true,
          "catenate_words": true,
          "preserve_original": true
        },
        "my_word_delimiter_search": {
          "type": "word_delimiter",
          "catenate_numbers": true,
          "catenate_words": true
        },
        "my_stemming" : {
          "type" : "stemmer",
          "language" : "minimal_english"
        },
        "shingle_filter": {
          "type": "shingle",
          "output_unigrams": true,
          "token_separator": ""
        },
        "stop_english": {
          "type": "stop",
          "stopwords": "_english_"
        },
        "stop_custom": {
          "type": "stop",
          "stopwords": ["product", "products", "&"]
        },
        "synonym_indextime": {
          "type" : "synonym",
          "synonyms" :[
            "body wash,shower gel",
            "nail polish, nail paint, nail enamel, nail lacquer",
            "cream, creme   ",
            "professional,  professionnel   ",
            "colour,  color   ",
            "face wash, facial wash, facial cleanser, face cleanser"
          ]
        }

      }
    }
  },
